;; Set variables used in other init files that are machine specific.
;; C-x C-w to save as init-local.el and change variables to match current machine.

(setq local-dropbox "")
(setq local-dropbox-notes (concat local-dropbox ""))
(setq local-aspell-exe "")
(setq local-desktop "")
(setq local-cygwin-bin "")
(setq local-cygwin-root "")

(provide 'init-local)
